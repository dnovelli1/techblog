<div class="container">
<form class="card blue-grey darken-1 login-form">
    <div class="row">
        <div class="input-field col s6">
            <input id="usernameLog" type="text" class="input"/>
            <label class="active" for="usernameLog">Username</label>
        </div>
    </div>
    <div class="row">
        <div class="input-field col s6">
            <input id="passwordLog" type="text" class="input"/>
            <label class="active" for="passwordLog">Password</label>
        </div>
    </div>
    <div>
        <button class="waves-effect waves-light btn" type="submit">Login</button>
        <div class="signup"><a class="text" href="/signup">Not a Member? Click to Sign Up!</a></div>
    </div>
</form>
</div>
<script src="/js/login.js"></script>